{
  "name": "graphql-functions-generator",
  "version": "1.0.0",
  "description": "Generate named graphql functions",
  "main": "src/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "build:clean": "rm -rf ./build",
    "start:dev": "npx nodemon",
    "start": "npm run build:clean && npm run build && node build/index.js",
    "generate": "npm run build:clean && npm run build && node build/index.js --gqlFilePath=./src/gqlFilePath --graphqlFilePath=./src/graphqlFilePath --outputDir=./generated/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gtAvenir/graphql-functions-generator.git"
  },
  "bugs": {
    "url": "https://github.com/gtAvenir/graphql-functions-generator/issues"
  },
  "homepage": "https://github.com/gtAvenir/graphql-functions-generator#readme",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.6.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/client-preset": "^4.0.1",
    "graphql": "^16.7.1",
    "urql": "^4.0.4"
  }
}
